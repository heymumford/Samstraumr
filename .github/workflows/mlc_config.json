{
  "aliveStatusCodes": [200, 202, 429, 403, 401, 410],
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://github.com/emumford/Samstraumr/(issues|pulls)/"
    },
    {
      "pattern": "^../../../"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^https?://.*\\.oup\\.com"
    },
    {
      "pattern": "^https?://.*bodleian\\.ox\\.ac\\.uk"
    },
    {
      "pattern": "^https?://.*treca\\.org"
    },
    {
      "pattern": "^https?://.*scribd\\.com"
    },
    {
      "pattern": "^https?://.*soundcloud\\.com"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ]
}